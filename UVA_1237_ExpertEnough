#include <iostream>
#include <stdio.h>
#include <vector>
#include <math.h>
#include <string>
#include <string.h>
using namespace std;

vector<pair<string, pair<int, int> > > data;

int main()
{
    int T, D, Q, cost, l, h;
    string str;
    cin >> T;
    for(int t=0; t<T; t++)
    {
        data.clear();
        cin >> D;
        for(int i=0; i<D; i++)
        {
            cin >> str >> l >> h;
            data.push_back(make_pair(str, make_pair(l, h)));
        }
        cin >> Q;
        for(int q=0; q<Q; q++)
        {
            cin >> cost;
            str = "";
            for(int i=0; i<D; i++)
            {
                //cout << cost << " " << data[i].second.first << " " << data[i].second.second << endl;
                if(cost >= data[i].second.first && cost <= data[i].second.second)
                {
                    if(str == "")
                    {
                        str = data[i].first;
                    }
                    else
                    {
                        str = "";
                        break;
                    }
                }
            }
            if(str == "")
            {
                cout << "UNDETERMINED" << endl;
            }
            else
            {
                cout << str << endl;
            }


        // Take Care: An Empty Line to separate 2 different test cases !!!
        if(t != T-1)
        {
            cout << endl;
        }
    }
    return 0;
}
